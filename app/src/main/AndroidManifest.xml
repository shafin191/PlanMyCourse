<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.csesd3.plan_my_course.planmycourse">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.QuizEntryActivity" />
        <activity android:name=".activity.QuizShowActivity" />
        <activity android:name=".activity.RoutineActivity" />
        <activity android:name=".activity.SyllabusActivity" />
        <activity android:name=".activity.SignInActivity" />
        <activity android:name=".activity.QuizActivity" />
        <activity android:name=".activity.SectionRoutineActivity" />
        <activity android:name=".activity.ShowRoutineActivity" />
        <activity android:name=".activity.ShowSyllabusActivity" />
        <activity android:name=".activity.CgpaCountActivity" />
        <activity android:name=".activity.CGPA_SesscionalActivity" />
        <activity android:name=".activity.CGPA_TheoryActivity" />
        <activity android:name=".activity.layout25"></activity>
    </application>

</manifest>
